{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "predev": "npm run seed:all",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "prestart": "npm run seed:all",
    "start": "node dist/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:users": "jest test/modules/users/automated/user.test.ts",
    "test:integration": "jest test/integration-tests/user-register-login.test.ts",
    "test:report:register": "node scripts/generate-test-report.js",
    "test:report:login": "node scripts/generate-login-test-report.js",
    "test:report:admin:register": "node scripts/generate-admin-register-test-report.js",
    "test:report:admin:login": "node scripts/generate-admin-login-test-report.js",
    "test:report:admin:complete": "node scripts/generate-admin-complete-test-report.js",
    "test:report:all": "node scripts/generate-all-test-reports.js",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "seed:books": "ts-node src/seeds/books.seed.ts",
    "seed:genres": "ts-node src/seeds/genres.seed.ts",
    "seed:all": "npm run seed:genres && npm run seed:books"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.6.1",
    "@types/supertest": "^6.0.3",
    "create-jest": "^30.2.0",
    "jest": "^30.2.0",
    "jest-html-reporter": "^4.3.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  }
}
